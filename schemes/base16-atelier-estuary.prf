ColorsUseSystem=Bfalse
ColorsUseMLintAutoFixBackground=Btrue
Editor.VariableHighlighting.Automatic=Btrue
Editor.NonlocalVariableHighlighting=Btrue
EditorCodepadHighVisible=Bfalse                          # no cell highlighting
EditorCodeBlockDividers=Btrue                            # mark cells with line
Editorhighlight-caret-row-boolean=Btrue
EditorRightTextLineVisible=Btrue
EditorRightTextLimitLineWidth=I1
ColorsText=C0x929181
ColorsBackground=C0x22221b
Colors_M_Keywords=C0x5f9182                      # violet
Colors_M_Comments=C0x6c6b5a
Colors_M_Strings=C0x7d9726                       # green
Colors_M_UnterminatedStrings=C0x9d6c7c           # dark red
Colors_M_SystemCommands=C0x5b9d48                # cyan
Colors_M_Errors=C0xba6236                        # red
Colors_HTML_HTMLLinks=C0x36a166                  # blue
Colors_M_Warnings=C0xa5980d                      # yellow
ColorsMLintAutoFixBackground=C0x5f5e4e
Editor.VariableHighlighting.Color=C0x5f5e4e
Editor.NonlocalVariableHighlighting.TextColor=C0xae7313  # orange
Editorhighlight-lines=C0x6c6b5a
Editorhighlight-caret-row-boolean-color=C0x5f5e4e
EditorRightTextLimitLineColor=C0x5f5e4e          # matlab cell lines
# MuPAD
Editor.Language.MuPAD.Color.operator=C0xae7313   # orange
Editor.Language.MuPAD.Color.function=C0x36a166   # blue
